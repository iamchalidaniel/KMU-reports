{
    "name": "kmu-discipline-desk",
    "version": "1.0.0",
    "description": "KMU Discipline Desk - Complete Desktop Application",
    "main": "frontend/main.js",
    "scripts": {
        "install-all": "cd frontend && npm install && cd ../backend && npm install",
        "dev": "npm run install-all && cd backend && npm run dev & cd frontend && npm run dev",
        "build": "node scripts/build-exe.js",
        "build-exe": "node scripts/build-exe.js",
        "package": "cd frontend && npm run package-win",
        "electron-dev": "cd frontend && npm run electron-dev",
        "deploy-pwa": "node scripts/deploy-pwa.js",
        "deploy-backend": "node scripts/deploy-pwa.js --backend",
        "fix-permissions": "node scripts/fix-permissions.js",
        "test-db": "node scripts/test-database.js"
    },
    "dependencies": {
        "chart.js": "^4.5.0",
        "clsx": "^2.1.1",
        "exceljs": "^4.4.0",
        "react-chartjs-2": "^5.3.0",
        "socket.io": "^4.8.1",
        "socket.io-client": "^4.8.1"
    },
    "devDependencies": {}
}
